{
  "scripts": {
    "start": "concurrently \"yarn start:server\" \"yarn start:web\"",
    "start:server": "cd amplify/backend/function/server/src && dotnet watch run",
    "start:web": "webpack serve --config=configs/webpack/dev.js",
    "build": "yarn build:web && yarn build:server",
    "build:web": "yarn run clean-dist &&BABEL_ENV=production webpack --config=configs/webpack/prod.js --progress",
    "build:server": "cd amplify/backend/function/server/src && dotnet build && dotnet publish -c release",
    "clean-dist": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
